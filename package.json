{
  "name": "mcpeapsunterstrichhd",
  "scheme": "mahd",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start:go": "expo start --https --go",
    "android:go": "expo start --android --https --go",
    "ios:go": "expo start --ios --https --go",
    "web:go": "expo start --web --https --go",
    "start:dev": "expo start --https --dev-client",
    "android:dev:dev": "expo start --android --dev-client",
    "ios:dev:dev": "expo start --ios --dev-client",
    "web:dev": "expo start --web --dev-client",
    "ios:dev:build:local:sim": "expo run:ios",
    "android:dev:build:local:sim": "expo run:android",
    "ios:dev:build:local:device": "expo run:ios --device",
    "android:dev:build:local:device": "expo run:android --device",
    "start:prod": "expo start --https --no-dev",
    "android:prod": "expo start --android --https --no-dev",
    "ios:prod": "expo start --ios --https --no-dev",
    "web:prod": "expo start --web --https --no-dev",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build": "expo export",
    "build:web": "expo export --platform web",
    "build:local:android": "expo prebuild --platform android",
    "build:local:ios": "expo prebuild --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:prod": "expo export --profile production",
    "build:dev": "expo export --profile development",
    "reset-project": "node ./scripts/reset-project.js",
    "doctor": "expo-doctor",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/cli": "0.22.7",
    "@expo/metro-runtime": "~4.0.0",
    "@expo/vector-icons": "^14.0.4",
    "@react-native/debugger-frontend": "^0.76.5",
    "@react-navigation/drawer": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "expo": "~52.0.20",
    "expo-asset": "~11.0.1",
    "expo-blur": "~14.0.1",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.6",
    "expo-font": "~13.0.2",
    "expo-haptics": "~14.0.0",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.14",
    "expo-splash-screen": "~0.29.18",
    "expo-status-bar": "~2.0.0",
    "expo-symbols": "~0.2.0",
    "expo-system-ui": "~4.0.6",
    "expo-updates": "~0.26.10",
    "expo-web-browser": "~14.0.1",
    "metro": "^0.81.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.6",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-syntax-highlighter": "^2.1.0",
    "react-native-v8": "^2.5.1",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "react-refresh": "^0.16.0",
    "v8-android-jit": "^11.1000.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^19.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.2",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.7.2"
  },
  "overrides": {
    "strip-ansi": "^7.1.0"
  },
  "private": true,
  "license": "MIT",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
